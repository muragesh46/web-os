    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "@components/*": ["src/components/*"],
      "@constants/*": ["src/constants/*"],
      "@store/*": ["src/store/*"],
      "@hoc/*": ["src/hoc/*"],
      "@windows/*": ["src/features/windows/*"],
      "@services/*": ["src/services/*"],
      "@hooks/*": ["src/hooks/*"],
